{
  "name": "{{ projectName }}",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "rxbot dev",
    "build": "rxbot build",
    "deploy": "rxbot deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.5.4",
    "vercel": "^37.14.0"
  },
  "dependencies": {
    "rxbot-cli": "{{packageVersion}}",
    "react": "^18.3.1",
    "dotenv": "^16.4.5",
    "react-reconciler": "^0.29.2",
    "@vercel/functions": "^1.5.0",
    "@types/react": "^18.3.12",
    "@rx-lab/common": "{{packageVersion}}",
    "@rx-lab/core": "{{packageVersion}}"
    {%- for adapter in adapters %}
      {%- if adapter === 'telegram' %},
    "@rx-lab/telegram-adapter": "{{packageVersion}}"
      {%- endif %}
    {%- endfor %}
    {%- if storage === 'upstash-redis' %},
    "@rx-lab/upstash-storage": "{{packageVersion}}"
    {%- elif storage === 'memory' %},
    "@rx-lab/storage": "{{packageVersion}}"
    {%- elif storage === 'file' %},
    "@rx-lab/file-storage": "{{packageVersion}}"
    {%- endif %}
  }
}
