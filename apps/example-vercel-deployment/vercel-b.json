{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "your-custom-build-command-here"
      }
    },
    { "src": ".rx-lab/**", "use": "@vercel/static" },
    { "src": "api/**/*.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/.rx-lab/(.*)", "dest": "/.rx-lab/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "outputDirectory": "."
}
